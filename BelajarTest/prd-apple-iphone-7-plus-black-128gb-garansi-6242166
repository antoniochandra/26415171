<!-- added by steven -->
<script type="text/javascript">
    jQuery(document).ready(function($){
        /**
         * below is just a hot fix for 'konsultasi email' matter
         * it is too expensive of a script -- waste, as consequences to request IAX-441
         * which enforce to show popup instead of rendering the helpdesk page
         *
         * TODO: NEED TO DELETE THIS! right after bugs in email consultation fixed
         */
        $('.emailCounsel a, .custBan a:first-child, .callUs a').attr('href','javascript:void(0);');
        $('li .contact-us-cs').attr('href','javascript:void(0);'); //XOP-10169

        var overlayPopup = {
            show: function(){

                $('.popupOverlay').css({
                    "top": 0,
                    "left": 0
                });
                $('.popupContainer').css({
                    "top": "50%",
                    "left": "50%"
                }).find('.sectionMap').html(function(index, html){
                    return html.replace(/(\<!--|--\>)/gi,'');
                });
            },
            hide: function(){
                $('.popupContainer, .popupOverlay').css({
                    "top": "-10000px",
                    "left": "-10000px"
                });
            }
        };

        $('section.content a').each(function(idx, item){
            var $item = $(item);
            var href = $item.attr('href');
            if(href != null) {
            	if(href.match(/helpdesk\/Qna\/goHelpMailSendForm\.do/gi) || href.match(/mailto:customersupport@elevenia\.co\.id/gi)){
                    $item.attr('href','javascript:void(0);');
                    $item.on('click', function(e){
                        e.preventDefault();
                        overlayPopup.show();
                    });
                }
            }
        });

        $(".custCenter .callUs a, .custBan a:first-child, ul.about .contact-us-cs").on('click', function(e){
            e.preventDefault();
            overlayPopup.show();
        });

        $(".popupContainer a.btnClose, .popupOverlay").on('click', function(e){
            e.preventDefault();
            overlayPopup.hide();
        });
        
      //MTD-242 START
      var screenWidth = $(window).width();

      if (screenWidth < 983) {
        $(".switch_version").addClass("mo");
      } else {
        $(".switch_version").removeClass("mo");
      }       
	 //MTD-242 END   
       
    });
  
</script>
<!-- popup end -->

