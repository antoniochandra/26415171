	(function() {
	    function rtbhouse_addon_load(){
	    	var prdNo = encodeURIComponent("6242166");
	    	var prdNm = encodeURIComponent("Apple iPhone 7 PLUS 128GB GRS Internasional 1th");
	    	var prdUrl = encodeURIComponent("http://www.elevenia.co.id/prd-apple-iphone-7-plus-128gb-grs-internasional-1th-6242166");
	    	var prdImgUrl = encodeURIComponent("http://cdn.elevenia.co.id/g/2/4/2/1/6/6/6242166_B_V4.jpg");
	    	var prdPrc = encodeURIComponent("14300100");
	    	var prdOldPrc = encodeURIComponent("15052700");
	    	var prdCtgrNo= encodeURIComponent("361");
	    	var prdCtgrNm = encodeURIComponent("Handphone & Smartphone");
	    	
	    	var imgSrc ="//tracker.creativecdn.com/4KDfeLACDg4QYXn8nojg/offers?id="+prdNo+"&name="+prdNm+"&url="+prdUrl+"&img_url="+prdImgUrl+"&price="+prdPrc+"&old_price="+prdOldPrc+"&category_id="+prdCtgrNo+"&category_name="+prdCtgrNm; 
	    	var img = document.createElement('img');
		    	 img.style.display = "none";
		    	 img.border = "0"
		    	 img.src = imgSrc;
		    	 img.width ="1";
		    	 img.height="1";	    	    
	    	    document.body.appendChild(img);
	    }
	    window.attachEvent ? window.attachEvent('onload', rtbhouse_addon_load) : window.addEventListener('load', rtbhouse_addon_load, false);
	})();
	
