        }else if(filterReviewType == "UB"){         
            _post.loadData(1);
        }else{
            _review.loadData('first',1,'');
            _post.loadData(1);
        }
    }
    
    function _fnPrdReporting(){
		var frm = document.frmLaporkan;
		var vrptPrdNo = "";
		var vrptPrdNm = "";
		var vrptRsnCd = "";
		var vrptRsnDtls = "";
		
		vrptPrdNo = frm.rptPrdNo;
		vrptPrdNm = frm.rptPrdNm;
		vrptRsnCd = frm.rptRsnCd;
		vrptRsnDtls = frm.rptRsnDtls;

		FormObjTrim(frm);
		if(!isRadio(frm.rptRsnCd,'Alasan'))return false;
		if(isBlank(frm.rptRsnDtls)){
			alert('Silahkan tulis pesan laporan');
			frm.rptRsnDtls.focus();
			return false;
		}

		strVan = getBanWordMessage(frm.rptRsnDtls.value);
		if( strVan != "")
		{
			alert("["+usedBanWordList(frm.rptRsnDtls.value)+"]"+strVan);
			frm.rptRsnDtls.focus();
			return;
		}
		
		if($('#rptRsnDtls').val().length > 300){
			alert("Hanya dapat menulis 300 karakter.");
			frm.rptRsnDtls.focus();
			return;
		}

		/* frm.action = "/community/AuthShoppingInfoAction/insertReportingProduct.do";
		frm.submit(); */
		
		$.ajax({
	        type : 'GET',
	        url : '/community/AuthShoppingInfoAction/insertReportingProduct.do',
	        data : {
	        	prdNo : vrptPrdNo.value, prdNm : vrptPrdNm.value, rptRsnCd : vrptRsnCd.value, rptRsnDtls : vrptRsnDtls.value
	        },
	        dataType : 'json',
			success : function(data) {
				if (data.Result == '00') {
					setTextPopUp3("Terima kasih atas laporan Anda!");
					_setPopupClosed();
				}else if (data.Result == '212') {
					setTextPopUp3("Anda telah melaporkan produk ini sebelumnya!");
					_setPopupClosed();
					return false;
				}else if (data.Result == '1') {
					setTextPopUp3("Hari ini Anda telah melaporkan produk sebanyak 5 kali.");
					_setPopupClosed();
 	                return false;
				}
			},
			error : function(xhr) {
				alert("Ada kesalahan sistem.", false);
				return false;
			}
	    });
	}
	
	function _setPopupClosed(){
		$('#alertPop').toggle();
		$('#dimmed').toggle();
		$('#frmLaporkan input[type="radio"').attr('checked',false)
		$('#rptRsnDtls').val("");
	}
	
	function gapAddToCart(){
		try{
			var $optionName = $("input[name='optionNm']");
			var optionName = (typeof $optionName != "undefined") ? $("input[name='optionNm']").val() : "";
			var gapPrdObj = {
					"name":"Apple iPhone 7 PLUS 128GB GRS Internasional 1th",
					"id":"6242166",
					"price":"14300100",
					"brand" :"Apple",
					"category":"Handphone & Smartphone - 361",
					"variant":optionName,
					"quantity":$("#totOrdQty").val()
				};
			var gapPrdDetailParams = {"productList":[gapPrdObj]};
			gap.addToCart(gapPrdDetailParams);
		}catch(e){console.log(e)}
		
	}
	
	try{
		//gap
		var gapPrdObj = {
				"name":"Apple iPhone 7 PLUS 128GB GRS Internasional 1th",
				"id":"6242166",
				"price":"14300100",
				"brand" :"Apple",
				"category":"Handphone & Smartphone - 361",
				"variant":""
			};
		var gapPrdDetailParams = {"productObj":gapPrdObj};
		gap.setProductDetail(gapPrdDetailParams);
		

		//store best item
		var sellerStoreItemList =[];
		$(".itemBoxArea ul li a").each(function(index){
			
   			
           	var positionCounter = index + 1;
           	
          		var $prdData = $(this);//$(this).find(".prdData").eq(0);
      			var searchPrdGapObj = {
      	     		   'name': $prdData.attr("data-prdnm"), 
      	    	       'id': $prdData.attr("data-prdno") ,
      	    	       'price': $prdData.attr("data-prdprc"),
      	    	       'brand': '',
      	    	       'category': '',                       
      	    	       'variant': '',                
      	    	       'list': 'Product Detail Page - Seller Store Item',
      	    	       'position': positionCounter
      		    	};
      			sellerStoreItemList.push(searchPrdGapObj);
      	    	$(this).click(function(){
      	    		 var gapPrdClickParams = {"productObj":searchPrdGapObj};
      	    		 gap.setProductClick(gapPrdClickParams);
      	    	});	   
		});
		
		if(sellerStoreItemList.length>0){
   	    	var searchPrdGapParams = {"productList":sellerStoreItemList};
   			gap.setProductImpression(searchPrdGapParams);			
   	    }
		
		var $btmBanner = $(".pdp_img");
		if(typeof $btmBanner != "undefined"){
			var $btmBannerImg = $btmBanner.find("img");
			var $btmBannerAnc = $btmBanner.find("a");
			if(typeof $btmBannerImg != "undefined"){
				var btmBannerImgAlt = $btmBannerImg.attr("alt");
				var btmBannerBnnrNo = $btmBannerAnc.attr("data-bnnrno");
				if(btmBannerImgAlt !="undefined" && btmBannerImgAlt != "" && btmBannerBnnrNo != "undefined" && btmBannerBnnrNo != ""){
					var gapBtmBannerList = [{
						'name' : btmBannerImgAlt,
						'id': btmBannerBnnrNo,
						'creative':'Product Detail Page - Bottom Banner1',
						'position':"1"
					}];
					
					var params = {"promoList":gapBtmBannerList};
					
					gap.setPromotionImpression(params);	
				}
				
				if(typeof $btmBannerAnc != "undefined"){
					$btmBannerAnc.click(function(){
						gap.setPromotionClick(params);
					});
					
				}
			}
			
			
		}
	}catch(e){console.log(e)}
	
	//XOP-7072
	if(false){
		$('.productOrder.benefitAreas').hide()
	}
	
	var comp = $('.productOrder.benefitArea').children();
	var flag = false;
	
	comp.find('dt').each(function( index ) {
		var value= comp.find('dd').eq(index).text().trim();
		if(value == ''){
			comp.find('dt').eq(index).hide();
			comp.find('dd').eq(index).hide();
			flag = true;
		} else {
			if(index!=0 && flag){
				comp.find('dt').eq(index).text('Benefit');
				flag = false;
			}
		}
	}); 
	
