    function downloadCoupon(cupnNo, signature){
		if(!funcCheckIsLogin()) {
			alert("Silahkan login terlebih dahulu untuk mendownload voucher ini");                                                                                                                                                                                          
			login();
			return;

		} else {
			var url = "/loyalty/CouponDownloadManagement/downloadSchedulerCouponEncript.do";
			
			$.ajax({
			type: "POST"
				,dataType: "JSON"
				,url: url
				,data:{'couponNo':cupnNo,'signatureKey':signature}
				,success: function(data){
					if (data.Result == '00'){
				   		alert("Voucher telah berhasil diunduh. Gunakan voucher ini untuk pembelian selanjutnya. Cek My elevenia untuk melihat voucher yang telah diunduh");
				   	} else if (data.Result == '207') {
				   		alert("Mohon maaf, saat ini voucher belum tersedia. Silakan mengunduh pada waktu yang telah ditentukan");
				   	} else if (data.Result == '208') {
				   		alert("Mohon maaf, saat ini voucher telah habis");
				   	} else if (data.Result == '209') {
				   		//alert("Kupon ini tidak untuk di-download.");
				   	} else if (data.Result == '210') {
				   		alert("Kupon dikeluarkan oleh penjual");
				   	} else if (data.Result == '201') {
				   		alert("Maaf, kamu telah mendownload voucher ini sebelumnya");
				   	} else if (data.Result == '202') {
				   		alert("Maaf, kamu telah mendownload voucher ini sebelumnya");
				   	} else if (data.Result == '203') {
				   		alert("Mohon maaf, saat ini semua kuantitas voucher telah habis");
				   	} else if (data.Result == '108') {
				   		alert("Mohon maaf, terjadi kesalahan sistem. Silahkan hubungi Customer Service kami");
				   	} else if (data.Result == '109') {
				   		alert("Saat ini tidak bisa mengunduh voucher. Silahkan hubungi Customer Service kami");
				   	} else if (data.Result == '300') {
				   		alert("Limit voucher saat ini telah habis. Silahkan coba kembali pada kesempatan selanjutnya");
				   	} else if (data.Result == '400') {	
				   		alert("Maaf saat ini Anda belum dapat mendownload voucher. Silakan mencoba download voucher beberapa saat lagi.");
				   	} else if (data.Result == '500') {
				   		var alertMsg = "Nomor telepon Anda belum terverifikasi.\nLakukan verifikasi  nomor telepon di My elevenia > Ubah Informasi Anggota\nuntuk dapat mendownload voucher ini.";
				   		if(confirm(alertMsg)){
				   			window.open("https://www.elevenia.co.id/register/confirmPassword.do?isSSL=Y&type=EDIT&CHOICEMENU=E01");
				   		}
				   	} else {
				   		alert("Terjadi kesalahan yang tidak diketahui. Mohon hubungi Customer Service kami");
				    }
				}
				,error: function(xhr){
					alert("Kegagalan sistem telah terjadi. \n Silakan hubungi administrator sistem Anda");
				}
			});
			
		}
		
	}

</script>


<script>
// 배송비 관련 스크립트 작업
jQuery(document).ready(function($){

	var etprsNm = "";
	
	/*check the visitor who comes from promotion page*/
	var historyURL = document.referrer;
	var promoYn = "N";
	var promoNO = "";
	
	if(promoYn == "Y"){
		if(historyURL.indexOf(promoNO)< 0 ){
			history.go(-1);  
		}
	}
	
    if($('select#etprsDlvSel option:selected').val()=="") {
    	$("#dlvEtprsCd").val('');
    	if($("#dlvEtprsCd").val()==""){
	    	$('#sellerSumInfoList li').remove();
        	$("#sellerSumInfoList").append("<li id=\"clearanceNotif1\">Biaya pengiriman tergantung pada berat dan alamat tujuan</li>");        
	    	$("#sellerSumInfoList").append("<li id=\"clearanceNotif1\">Produk dikirim dari luar negeri dan memerlukan waktu hingga <b>60 hari kalender (estimasi) untuk sampai ke alamat tujuan tergantung metode pengiriman.</b></li>");     
	    	$("#sellerSumInfoList").append("<li id=\"clearanceNotif1\"><b>Produk dalam pengiriman tidak dapat dibatalkan.</b></li>");
    	}
    }
    
    // 배송비 노출 변경
    $("#etprsDlvSel").change(function(){
        //alert($("#etprsDlvSel").val());
        var etprsInfo   = $("#etprsDlvSel").val().split(",");
        etprsNm = $("#etprsDlvSel option:selected").text(); // JneAddInfo 20140205

        if(etprsInfo.length == 6){
            if(etprsInfo[1]>0){
                $("#dlvRp").html("Rp "+etprsInfo[1]+" ~");  
            }else{
                $("#dlvRp").html("");
            }
            $("#dlvEtprsCd").val(etprsInfo[0]);
            $("#dlvDtlsMthdCd").val(etprsInfo[2]);
            $("#drtDlvFeeOptClfCd").val(etprsInfo[3]);
            $("#dlvMthdCd").val(etprsInfo[4]);
            customClearanceNotif();//XOP-843
            
            if($("#dlvDtlsMthdCd").val() == "01") { //Regular Delivery 인 경우
                $("#conditionText01").show();
                $("#conditionText02").hide();
                $("#conditionText03").hide();
            } else if($("#dlvDtlsMthdCd").val() == "02") {  //Express Delivery 인 경우
                $("#conditionText02").show();
                $("#conditionText01").hide();
                $("#conditionText03").hide();
            } else if($("#dlvDtlsMthdCd").val() == "03") {  //Direct Delivery 인 경우
                $("#conditionText03").show();
                $("#conditionText01").hide();
                $("#conditionText02").hide();
            } else {
                $("#conditionText01").hide();
                $("#conditionText02").hide();
                $("#conditionText03").hide();
            }
            
            var freeshippingYN = etprsInfo[5];
            if(freeshippingYN == 'Y') {
                $("#divFreeShipping").show();
            } else {
                $("#divFreeShipping").hide();
            }
            
        }else{
        	if($('select#etprsDlvSel option:selected').val()=="") {
            	$("#dlvEtprsCd").val('');
            	if($("#dlvEtprsCd").val()==""){
        	    	$('#sellerSumInfoList li').remove();
                	$("#sellerSumInfoList").append("<li id=\"clearanceNotif1\">Biaya pengiriman tergantung pada berat dan alamat tujuan</li>");        
        	    	$("#sellerSumInfoList").append("<li id=\"clearanceNotif1\">Produk dikirim dari luar negeri dan memerlukan waktu hingga <b>60 hari kalender (estimasi) untuk sampai ke alamat tujuan tergantung metode pengiriman.</b></li>");     
        	    	$("#sellerSumInfoList").append("<li id=\"clearanceNotif1\"><b>Produk dalam pengiriman tidak dapat dibatalkan.</b></li>");
            	}
            }
        }
        
        //JneAddInfo
        /* if($("#jneAddInfo").length > 0){
            if(etprsNm.indexOf("JNE") >= 0){
                $("#jneAddInfo").show();
            } else {
                $("#jneAddInfo").hide();
            }
        } */
    });

    $('#dlvEtprsNm').val(etprsNm);

    // 숫자만 등록
    $("#showW").keydown(function(e){
        if(e.keyCode == 46 || e.keyCode == 8 || e.keyCode == 9 || e.keyCode == 27 || e.keyCode == 13
                || (e.keyCode == 65 && e.ctrlKey == true) || (e.keyCode >= 48 && e.keyCode <= 57)){

        } else {
            return false;;
        }
    });

    $("#showW").keyup(function(e){
        if(e.keyCode == 46 || e.keyCode == 8 || e.keyCode == 9 || e.keyCode == 27 || e.keyCode == 13
                || (e.keyCode == 65 && e.ctrlKey == true) || (e.keyCode >= 48 && e.keyCode <= 57)){
            var weight = jQuery("#showW").val();
            jQuery("#totalW").html(" " +weight+ ".00 ");
            fn_calcDelCost();
        } else {
            return false;;
        }
    });

})
    function fn_decWeight(){
        var weight = jQuery("#showW").val();
        var delCst   = jQuery("#dlvCity").val();    // 배송비

        if(weight > 1){
            weight--;

            jQuery("#showW").val(weight);
            jQuery("#totalW").html(" " +weight+ ".00 ");

            fn_calcDelCost();
        }
    }

    function fn_incWeight(){
        var weight = jQuery("#showW").val();
        var delCst   = jQuery("#dlvCity").val();    // 배송비
        weight++;

        jQuery("#showW").val(weight);
        jQuery("#totalW").html(" " +weight+ ".00 ");

        fn_calcDelCost();
    }
    // 택배사에 따른 하위 도시명 찾기
    function fn_searchDlvProvince(){
        var dlv_etprs_cd = jQuery("#popDlvSel").val();
        var tmplSeq = jQuery("#dlvTmpltSeq").val();

        jQuery("#showW").val(1);
        jQuery("#totalW").html(" 1.00 ");
        jQuery("#avgDelivery").html("");
        jQuery("#totalDelCost").html(" 0 ");

        jQuery.ajax({
              type      : "post"
            , url       : "/product/ProductDetailAction/getDlvProvinceList.do"
            , data      : {dlv_etprs_cd:dlv_etprs_cd, tmplSeq:tmplSeq }
            , dataType  : "json"
            , success   : function(response, status, request) {
                //alert(jQuery("#dlvProvince option").size());
                if(jQuery("#dlvProvince option").size() > 1){   // 셀렉트 박스 초기화
                    for(var i=1; i< jQuery("#dlvProvince option").size(); i++){
                        jQuery("#dlvProvince option").not("[value='']").remove();
                    }
                }
                if(jQuery("#dlvCity option").size() > 1){   // 셀렉트 박스 초기화
                    for(var i=1; i< jQuery("#dlvCity option").size(); i++){
                        jQuery("#dlvCity option").not("[value='']").remove();
                    }
                }
                //alert(jQuery("#dlvCity option").size());//dlvCity
                jQuery(response.completelist).each(function(index, item){
                    var selectInfo = "<option value='"+item.province+"'>"+item.province+"</option>"
                    jQuery("#dlvProvince").append(selectInfo);
                });
                jQuery("#dlvProvince").trigger("chosen:updated")
            }
            , error     : function(xmlHttpRequest, txtStatus, errorThrown) {

            }
        });
    }

    function fn_getDlvCityList(){

        var dlv_etprs_cd = jQuery("#popDlvSel").val();
        var tmplSeq = jQuery("#dlvTmpltSeq").val();
        var province    = jQuery("#dlvProvince").val();
        var dlv_etprs_nm = jQuery("#popDlvSel option:selected").text();
        //alert(dlv_etprs_nm);
        if(jQuery("#dlvCity option").size() > 1){   // 셀렉트 박스 초기화
            for(var i=1; i< jQuery("#dlvCity option").size(); i++){
                jQuery("#dlvCity option").not("[value='']").remove();
            }
        }
        //alert(province);
        if(province != ""){
            jQuery("#dlvCity").html("<option value=''>Loading...</option>").trigger("chosen:updated");
            jQuery.ajax({
                  type      : "post"
                , url       : "/product/ProductDetailAction/getDlvCityList.do"
                , data      : {dlv_etprs_cd:dlv_etprs_cd, tmplSeq:tmplSeq ,province:province, dlv_etprs_nm:dlv_etprs_nm}
                , dataType  : "json"
                , success   : function(response, status, request) {
                    jQuery("#dlvCity").html("<option value=''>Pilih Kabupaten/Kota</option>");
                    jQuery(response.completelist).each(function(index, item){
                        var selectInfo = "<option value='"+item.tariffDlvCst+","+item.basicIncCst+","+item.etdReg+"'>"+item.cityNm+ " "+ item.district+"</option>";
                        jQuery("#dlvCity").append(selectInfo);
                        jQuery("#dlvCity").trigger("chosen:updated");
                    });
                }
                , error     : function(xmlHttpRequest, txtStatus, errorThrown) {
                }
            });
        }else{
            jQuery("#dlvCity").html("<option value=''>Pilih Kabupaten/Kota</option>").trigger("chosen:updated");
        }
        
    }

    // 배송비 계산
    function fn_calcDelCost(){
        var delInfos    = $("#dlvCity").val().split(",");
        var delCst   = 0;   // 배송비
        var weight = jQuery("#showW").val();
        var totalDelCost = 0;
        var incStdCst   = 0;
        var avgDelevery = "";

        if(delInfos.length == 3){
            delCst      = delInfos[0];
            incStdCst   = delInfos[1];
            if(delInfos[2] != null){
                avgDelevery = delInfos[2];
            }
        }

        if(delCst == null || delCst == "0" || weight == null || weight < 1){
            return false;
        }
        if(weight > 1){
            totalDelCost = Number(delCst) + Number(incStdCst * (weight-1));
        } else {
            totalDelCost = delCst;
        }

        jQuery("#totalDelCost").html(" " +_setCssPrice(totalDelCost)+" ");
        if(avgDelevery != null && avgDelevery != "" && avgDelevery != undefined && avgDelevery != "undefined"){
            jQuery("#avgDelivery").html("<p class=\"dotList\">Rata-rata waktu pengiriman barang <em class=\"pointCBrw\">"+avgDelevery+"</em> hari</p>");
        } else {
            jQuery("#avgDelivery").html("");
        }
    }
    // 택배사 변경시
    function fn_popDvlChange(){
        if(jQuery("#dlvCity option").size() > 1){   // 셀렉트 박스 초기화
            jQuery("#dlvCity option").not("[value='']").remove();
        }
        fn_getDlvCityList();
        fn_calcDelCost();
    }
// 배송 관련 스크립트 끝

// 단골 등록 스크립트 시작
function fn_popRegistStore(){
    /* var left = jQuery(window).scrollLeft() + ((jQuery(window).width() - 530) / 2);
    var top = jQuery(window).scrollTop() + 50;
    jQuery("#dimmedLayerAjax").css({'left' : left, 'top' :top});
    jQuery("#popStoreSuccess").css({'left' : left, 'top' :top}); */
    if(funcCheckIsLogin()){
        jQuery("#popRegularShop").css({
            "marginLeft" : (200)
        }).show();
        //jQuery("#popRegularShop").show();
        fn_dimmedLayer();
        jQuery("#dimmedLayerAjax").fadeIn("fast");
    } else {
        alert("silakan log in.");
        //openLoginPopup('01');
    }

}

function fn_closeLayer(layerNm){
    jQuery("#"+layerNm+"").hide();
    jQuery("#dimmedLayerAjax").fadeOut();
    if(layerNm == "popStoreSuccess"){
        document.location.reload();
    }

}
// 딤드 레이어
function fn_dimmedLayer(gubun){
    var documentW = jQuery(document).width();
    var documentH = jQuery(document).height();
    var scrollTop = jQuery(window).scrollTop()+200;
    var backgroundColor = "#BDBDBD";

    var dimmedHTML = "";

    if(gubun != null && gubun != "" ){
        dimmedHTML  = "<div class='dimmed_1207'><div></div></div>";
    } else {
        dimmedHTML = "<div id=\"dimmedLayerAjax\" style=\"width:100%; height:"+documentH+"px; position:absolute; top:0; left:0; filter:Alpha(opacity=50);opacity:0.5; background:"+backgroundColor+"; z-index:100;display:none;\"></div>";
    }

    jQuery("body").append(dimmedHTML);

}

function fn_registerPopStore(){
    var checkedYn           = jQuery("#agreeStoreInfo").is(":checked");
    var seller_mem_no   = jQuery("#seller_mem_no").val();
    var sellerHomeNo        = jQuery("#sellerHomeNo").val();

    /* if(!checkedYn){  // Not Checked
        //alert("수신동의 체크 후, 확인 버튼을 클릭하시면 \n단골 등록 및 단골쿠폰 다운로드가 가능합니다.");
        alert("Anda dapat berlangganan dan men-download voucher \nsetelah mencentang persetujuan dan mengklik tombol 'setuju'");
        return;
    } */
    jQuery.ajax({
          type      : "post"
        , url       : "/product/ProductDetailAction/insertPopStore.do"
        , data      : {seller_mem_no:seller_mem_no, mgCustClfCd:"101", sellerHomeNo:sellerHomeNo}
        , dataType  : "json"
        , success   : function(response, status, request) {
            //alert(response.rstCd);
            if(response.rstCd == "88"){
                alert('silakan log in.');
            } else if (response.rstCd == "00"){
                fn_popStoreSuccess();
            } else if (response.rstCd == "99"){
                alert("Error terjadi pada saat sedang mendaftar."); // 등록중 오류
            } else {
                alert("Store sudah terdaftar.");
                fn_closeLayer("popRegularShop");
                document.location.reload();
            }
        }
        , error     : function(xmlHttpRequest, txtStatus, errorThrown) {
            alert("Error terjadi pada saat sedang mendaftar."); // 등록중 오류
        }
    });
}

function fn_popStoreSuccess(){
    jQuery("#storeInsBtn").hide();
    jQuery("#popRegularShop").hide();
    jQuery("#popStoreSuccess").css({
        "marginLeft" : (200)
    }).show();
}

// 회원 팝업에서 회원 구매하기 버튼 클릭 시 실행 되는 스크립트
function actionForLoginOK() {
    jQuery('body').removeClass('overH');
    if(jQuery("#buyYn").val() == 'Y'){
        directNonLogin();
    } else {
        document.location.reload();
    }
}

//회원 팝업에서 비회원 구매하기 버튼 클릭 시 실행 되는 스크립트
function actionForNonLoginOK() {
    jQuery('body').removeClass('overH');
    if(jQuery("#buyYn").val() == 'Y'){
        directNonLogin();
    } else {
        document.location.reload();
    }
}

</script>


