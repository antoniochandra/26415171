    // Magnify Image
    //jQuery.noConflict();
    

    function chkFirstLogin(){
    var result = false;
    jQuery.ajax({
            type : "post"
            , async : false
            , url : "/register/personalinfomation/chkFirstLogin.do"
            /* , data : {loginId:memid} */
            , success : function(response, status, request) {
                            var msg = request.responseText;
                            if(msg=='X'){
                                location.href = "/register/confirmPassword.do?isSSL=Y&type=EDIT&CHOICEMENU=E01";    
                            }
                        }
            , error : function(xmlHttpRequest, txtStatus, errorThrown) {

                        }
        });
    }
    

