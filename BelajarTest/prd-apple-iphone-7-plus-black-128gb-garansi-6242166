<script type="text/javascript">

	// changed Coming Soon page that every footer link <a href>
	jQuery("#footer").find("a").each(function() {
		var a_href = jQuery(this).attr("href");
		var comingSoon = "http://www.elevenia.co.id/commons/ComingSoonAction/comingSoon01.do";
		if(a_href == "#") {
			jQuery(this).attr("href", comingSoon);
			jQuery(this).attr("target", "");
		}
	});
	
	var protocol = location.protocol;
	var pathname = location.pathname;
	
	var url = protocol+"//m.elevenia.co.id/index.do?switch=1";
	if(protocol == "https:") {
    	url = protocol+"//m.elevenia.co.id/index.do?switch=1&ref="+pathname;
    }
	jQuery("#switch-mobile-url").attr("href",url);
</script>

