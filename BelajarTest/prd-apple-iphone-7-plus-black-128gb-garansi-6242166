	(function() {
	    function rtbhouse_load(){
	    	 var iframe = document.createElement('iframe');
	    	    iframe.style.display = "none";
	    	    iframe.src = "//asia.creativecdn.com/tags?id=pr_4KDfeLACDg4QYXn8nojg_offer_6242166";
	    	    iframe.width ="1";
	    	    iframe.height="1";
	    	    iframe.scrolling="no";
	    	    iframe.frameBorder="0";	    	    
	    	    document.body.appendChild(iframe);
	    }
	    window.attachEvent ? window.attachEvent('onload', rtbhouse_load) : window.addEventListener('load', rtbhouse_load, false);
	})();
	 
