    function _fnGoodBadAread(){

        var url = "/contents/embed/sr/embedGoodBad.do";

        var addrCnt = 0;
        var freeReturn = "N";
        var objFrm = document.frmMain;

        var params = "prdNo=" + objFrm.prdNo.value;

        jQuery.ajax({
            type:"post",
            url:url,
            data:params,
            dataType: "html",
            success:function(htmlData){
                jQuery('#goodBadArea').html(htmlData);
            },
            error:function(e){
                alert(e.responseText);
            }
        });
    }

    // ??
    function _fnReviewCount(){

        var url = "/contents/sr/getShortReviewCount.do";

        var addrCnt = 0;
        var freeReturn = "N";
        var objFrm = document.frmMain;

        var params = "prdNo=" + objFrm.prdNo.value;

        jQuery.ajax({
            type:"post",
            url:url,
            data:params,
            dataType:"json",
            success:function(jsonData){
                var tempStr = "<span>(<strong>"+jsonData+"</strong>)</span>";
                jQuery('#reviewCount').html(tempStr);
            },
            error:function(e){
                alert(e.responseText);
            }
        });
    }

