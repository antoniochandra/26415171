<script type="text/javascript">
    //prdDescIfrm
    var fframe = $('#prdDescIfrm');
    fframe.load(function() {
        if(fframe.contents().find('img').width() > 940){
            fframe.contents().find('img').width(940);
        }
    });
    
    
    $(function(){
        if($(".shockingDealsArea .countdown").length >0){
            var counterTimeInterval;
            var currentDateTime="2016-11-02-22-40-08";
            var dateParts= currentDateTime.split("-");
            var jCurrentDateTime=new Date(dateParts[0],dateParts[1]-1,dateParts[2],dateParts[3],dateParts[4],dateParts[5]);
            setInterval(function () {
                jCurrentDateTime.setSeconds(jCurrentDateTime.getSeconds() +1 );         
            }, 1000);
            
            
            var countdownSelector= $(".shockingDealsArea .countdown");
            var endDateTime= countdownSelector.attr("end-date");
        
            var jdateParts= endDateTime.split("-");
            var jEndDateTime=new Date(jdateParts[0],jdateParts[1]-1,jdateParts[2],jdateParts[3],jdateParts[4],jdateParts[5]);
            clearInterval(counterTimeInterval);
            setCountdownTimer(countdownSelector,jCurrentDateTime,jEndDateTime,"shocking");
            counterTimeInterval=setInterval(function () {                   
                setCountdownTimer(countdownSelector,jCurrentDateTime,jEndDateTime,"shocking");
            }, 1000);
        }
        
        if($(".dailyDealsArea .countdown").length >0){
            var counterTimeInterval;
            var currentDateTime="2016-11-02-22-40-08";
            var dateParts= currentDateTime.split("-");
            var jCurrentDateTime=new Date(dateParts[0],dateParts[1]-1,dateParts[2],dateParts[3],dateParts[4],dateParts[5]);
            setInterval(function () {
                jCurrentDateTime.setSeconds(jCurrentDateTime.getSeconds() +1 );         
            }, 1000);
            
            
            var countdownSelector= $(".dailyDealsArea .countdown");
            var endDateTime= countdownSelector.attr("end-date");
        
            var jdateParts= endDateTime.split("-");
            var jEndDateTime=new Date(jdateParts[0],jdateParts[1]-1,jdateParts[2],jdateParts[3],jdateParts[4],jdateParts[5]);
            clearInterval(counterTimeInterval);
            setCountdownTimer(countdownSelector,jCurrentDateTime,jEndDateTime,"daily");
            counterTimeInterval=setInterval(function () {                   
                setCountdownTimer(countdownSelector,jCurrentDateTime,jEndDateTime, "daily");
            }, 1000);
        }
        function setCountdownTimer(selector,startDtObj,endDtObj, dealType){
            
            var current_date =startDtObj.getTime();
            var target_date = endDtObj.getTime();
                    
            var seconds_left = (target_date - current_date) / 1000;
         
            
            days = parseInt(seconds_left / 86400);
            seconds_left = seconds_left % 86400;
             
            hours = parseInt(seconds_left / 3600);
            seconds_left = seconds_left % 3600;
             
            minutes = parseInt(seconds_left / 60);
            seconds = parseInt(seconds_left % 60);
            
            if(hours<10){
                hours= "0"+hours;
            }
            
            if(minutes < 10){
                minutes="0"+minutes;
            }
            if(seconds < 10){
                seconds = "0" +seconds;
            }         
            
            var hoursString= hours.toString();
            var minutesString=minutes.toString();
            var secondsString= seconds.toString();
           
            
            if(seconds_left <=0 ){
                 /* selector.html("0 day, 00:00:00 Tersisa"); */ 
                 /* $("#divTime").html("00:00:00 Tersisa");  */
                 if(dealType=="shocking"){
                    $(".shockingDealsArea").hide();
                 }
                 if(dealType=="daily"){
                    $(".dailyDealsArea").hide();
                 }
            }else{
                /*  selector.html(days + " day(s), " + hours + ":"
                        + minutes + ":" + seconds+ " Tersisa"); */                      
                if(dealType=="shocking"){
                     $(".shockingDealsArea #divTime").html(days);
                 }
                if(dealType=="daily"){
                     $(".dailyDealsArea #divTime").html(parseInt(hours));
                }
                
            }
            
            
        }//end of function countdowntimer
        
        
        //XOP-843
        customClearanceNotif();
        
        $("img[alt=question_mark]").hover(function() {
            var mP = $(this).offset();
            $('#question_info').css({top:mP.top+25,left:mP.left-(mP.left/2)+6,display:'block'});
            $('#question_info ul#'+$(this).attr('view')).css('display','block');
            $('#question_info div').css('left',mP.left/2);
        }, function() {
            $('#question_info ul#'+$(this).attr('view')).css('display','none');
            $('#question_info').css({display:'none'});
        });
        
        
        
    });
    
    function customClearanceNotif(){
        if($("#clearanceNotif").length > 0){
            $("#clearanceNotif").remove();
            $("#noAWBNotif").remove();
            $("#disputeNotif").remove();
        }
        
        /*
         * 00010 - Normal Air Mail
         * 00011 - Registered Air Mail
         * 00012 - EMS
         * */
         
	    if($('select#etprsDlvSel option:selected').val()=="00010,3.000,,,01,N" || $("#dlvEtprsCd").val()=="00010"){
       		$('#sellerSumInfoList li').remove();
         	$("#sellerSumInfoList").append("<li id=\"clearanceNotif\">Biaya pengiriman tergantung pada berat dan alamat tujuan</li>");        
	    	$("#sellerSumInfoList").append("<li id=\"clearanceNotif\">Produk dikirim dari luar negeri dan memerlukan waktu hingga <b>60 hari kalender (estimasi) untuk sampai ke alamat tujuan tergantung dari negara asal pengiriman.</b></li>");        		   
	    	$("#sellerSumInfoList").append("<li id=\"clearanceNotif\"><b>Produk dalam pengiriman tidak dapat dibatalkan.</b></li>");
	    	$("#sellerSumInfoList").append("<li id=\"clearanceNotif\"><b>Pengiriman luar negeri menggunakan Airmail/EMS dapat dikenakan pajak impor oleh bea cukai yang akan ditagihkan kepada pembeli. elevenia tidak akan melakukan pengembalian dana terhadap pembeli yang menolak membayar pajak dan/atau mengambil produknya sendiri di kantor pos.</b></li>");        
	    	$("#sellerSumInfoList").append("<li id=\"clearanceNotif\">Pengiriman menggunakan Normal Airmail tidak menyediakan nomor resi pengiriman dan <b>tidak dapat ditelusuri keberadaannya.</b></li>");        		   
	    	$("#sellerSumInfoList").append("<li id=\"clearanceNotif\">Produk yang hilang dan rusak saat pengiriman tidak menjadi tanggung jawab elevenia dan penjual.</li>");
	    }
        
        if($('select#etprsDlvSel option:selected').val()=="00011,2.000,,,01,N" || $('select#etprsDlvSel option:selected').val()=="00012,1.000,,,01,N" || $("#dlvEtprsCd").val()=="00011" || $("#dlvEtprsCd").val()=="00012"){
        	$('#sellerSumInfoList li').remove();
