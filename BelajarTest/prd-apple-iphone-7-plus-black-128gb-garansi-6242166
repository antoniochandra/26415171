     function classSet_add(){
        jQuery("div.add_prod table tr").removeClass();
        jQuery("div.add_prod table tr:last").addClass("last");
     }

     document.onreadystatechange=fnStartInit;
     var prdDescLoadFlag = false;

     /* XDR-89 2015.02.11 Akira 
     문서 로딩상태 => document.readyState; 
     uninitialized - 아직 로딩 시작 되자 않음
     loading - 로딩중
     interactive - 충분히 로드되고 사용자가 상호 작용 할수 있음
     complete - 로딩 완료 */
    function fnStartInit(){
       if (document.readyState=="interactive" && !prdDescLoadFlag) {
           prdDescLoad();
       }
    }

    var scrollingChkListener = null;

    function chkScrolling(){
        var nowY = 0;
        try{
            nowY = document.all ? (!document.documentElement.scrollTop ? document.body.scrollTop : document.documentElement.scrollTop) : (window.pageYOffset ? window.pageYOffset : window.scrollY);
        }catch(e){
        }

        if(nowY != 0){
            if(scrollingChkListener != null)
                clearInterval(scrollingChkListener);

            fnStartInit();
        }
    }

    function prdDescLoad(){
        if(!prdDescLoadFlag){
            prdDescLoadFlag = true;
            var prdDescIfrm = document.getElementById("prdDescIfrm");
            prdDescIfrm.src = "/product/SellerProductDetail/getSellerProductDetailDesc.do?prdNo=" + 6242166;
        }
    }

    scrollingChkListener = window.setInterval("chkScrolling()", 100);

</script>

