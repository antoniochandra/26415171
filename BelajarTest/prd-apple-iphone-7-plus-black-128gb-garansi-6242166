    var filterReviewType = '';
    function prodPurchaseReview(){
        filterReviewType = $("#filterReviewType option:selected").val();
    }
    
    function prodPurchaseReviewLoadEvlPnt(){
        var pntVal = $("#prodPurchaseReviewPnt option:selected").val();
        var prdRevList = [-1,0,1,2,3];
        var purRevList = [11,12,13,14,15];
        $("[name=review_param] [name=prdEvlPnt]").val(prdRevList[pntVal]);
        $("[name=buypost_param] [name=prdEvlPnt]").val(purRevList[pntVal]);
        
        $("[name=review_param] [name=prdEvlPnt]").val(pntVal);
        $("[name=buypost_param] [name=prdEvlPnt]").val(pntVal);
        
        if(filterReviewType == "UP"){  
            _review.loadData('first',1,'');
        }else if(filterReviewType == "UB"){
            _post.loadData(1);
        }else{
            _review.loadData('first',1,'');
            _post.loadData(1);
        }
        
    }
    
    function prodPurchaseReviewLoadGrade(){
        var grdVal = $("#prodPurchaseReviewGrd option:selected").val();
